{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/client/src",
  "monorepo": true,
  "root": "apps/client",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/client/tsconfig.app.json"
  },
  "projects": {
    "client": {
      "type": "application",
      "root": "apps/client",
      "entryFile": "main",
      "sourceRoot": "apps/client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client/tsconfig.app.json"
      }
    },
    "client-product": {
      "type": "application",
      "root": "apps/client-product",
      "entryFile": "main",
      "sourceRoot": "apps/client-product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-product/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    }
  }
}